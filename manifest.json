{
    "name": "Back in a Minute",
    "description": "Leave messages on your screen upon leaving",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "show tab inspector"
    }
}